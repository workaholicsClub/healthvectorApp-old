<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- %d - число; для английского языка множественные формы надо продублировать -->
    <plurals name="numberOfTimesInADay">
        <!-- форма для ед. числа, например, "1 раз в день" -->
        <item quantity="one">%d раз в день</item>
        <!-- форма для для числительных 2, 3, 4, например, "4 раза в день" -->
        <item quantity="few">%d раза в день</item>
        <!-- для языков, в которых числительные не имеют особой формы (как в английском языке) -->
        <item quantity="other">%d раз в день</item>
        <!-- форма для числительных 11-99, например, "11 раз в день" -->
        <item quantity="many">%d раз в день</item>
        <!-- форма для нуля, например, "0 раз в день" -->
        <item quantity="zero">%d раз в день</item>
    </plurals>
    <!-- %d - число; для английского языка множественные формы надо продублировать -->
    <plurals name="numberOfAddedEvents">
        <!-- форма для ед. числа, например, "Добавлено 1 событие" -->
        <item quantity="one">Добавлено %d событие</item>
        <!-- форма для для числительных 2, 3, 4, например, "Добавлено 4 события" -->
        <item quantity="few">Добавлено %d события</item>
        <!-- для языков, в которых числительные не имеют особой формы (как в английском языке) -->
        <item quantity="other">Добавлено %d событий</item>
        <!-- форма для числительных 11-99, например, "Добавлено 11 событий" -->
        <item quantity="many">Добавлено %d событий</item>
        <!-- форма для нуля, например, "Добавлено 0 событий" -->
        <item quantity="zero">Добавлено %d событий</item>
    </plurals>
    <!-- %d - число; для английского языка множественные формы надо продублировать -->
    <plurals name="numberOfPoints">
        <!-- форма для ед. числа, например, "1 балл" -->
        <item quantity="one">%d балл</item>
        <!-- форма для для числительных 2, 3, 4, например, "4 балла" -->
        <item quantity="few">%d балла</item>
        <!-- для языков, в которых числительные не имеют особой формы (как в английском языке) -->
        <item quantity="other">%d баллов</item>
        <!-- форма для числительных 11-99, например, "11 баллов" -->
        <item quantity="many">%d баллов</item>
        <!-- форма для нуля, например, "0 баллов" -->
        <item quantity="zero">%d баллов</item>
    </plurals>
</resources>
